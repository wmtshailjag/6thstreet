{
    "compilerOptions": {
        "baseUrl": "./",
        "paths": {
            "Style/*": [
                "src/style/*"
            ],
            "Component/*": [
                "src/component/*"
            ],
            "Route/*": [
                "src/route/*"
            ],
            "Store/*": [
                "src/store/*"
            ],
            "Util/*": [
                "src/util/*"
            ],
            "Query/*": [
                "src/query/*"
